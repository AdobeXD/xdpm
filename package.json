{
  "name": "@adobe/xdpm",
  "version": "2.0.0",
  "description": "Adobe XD CLI Plugin Manager Utility",
  "main": "index.js",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "bin": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdobeXD/xdpm.git"
  },
  "keywords": [
    "Adobe",
    "XD",
    "Plugin",
    "Manager",
    "Developer",
    "CLI"
  ],
  "author": "Adobe",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/AdobeXD/xdpm/issues"
  },
  "homepage": "https://github.com/AdobeXD/xdpm#readme",
  "dependencies": {
    "chokidar": "^2.0.4",
    "cli": "^1.0.1",
    "debounce": "^1.2.0",
    "ignore-walk": "^3.0.1",
    "mkdirp": "^0.5.1",
    "shelljs": "^0.8.2",
    "yazl": "^2.4.3",
    "@oclif/command": "^1.5.12",
    "@oclif/config": "^1.12.11"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.3",
    "@oclif/plugin-help": "^2.1.6",
    "eslint": "^5.15.3",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^8.0.2"
  },
  "files": [
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src",
    "/yarn.lock"
  ],
  "oclif": {
    "commands": "./commands",
    "bin": "./bin/run",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
